        "import pandas as pd\n",
        "\n",
        "def count_unique_subjects(teacher: pd.DataFrame) -> pd.DataFrame:\n",
        "    return teacher.groupby('teacher_id')['subject_id'].nunique().reset_index().rename(columns={'subject_id':'cnt'})"
