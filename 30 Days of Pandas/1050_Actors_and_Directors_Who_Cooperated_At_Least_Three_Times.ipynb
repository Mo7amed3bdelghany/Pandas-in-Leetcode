        "\n",
        "def actors_and_directors(actor_director: pd.DataFrame) -> pd.DataFrame:\n",
        "    df = actor_director.groupby(['actor_id','director_id']).count().reset_index()\n",
        "    return df[df['timestamp']>=3].drop('timestamp',axis=1)\n",
