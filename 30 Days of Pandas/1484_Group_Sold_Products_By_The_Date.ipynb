      "source": [
        "import pandas as pd\n",
        "\n",
        "def categorize_products(activities: pd.DataFrame) -> pd.DataFrame:\n",
        "\n",
        "    df = activities.groupby('sell_date')['product'].agg(['nunique', lambda x: ','.join(sorted(set(x)))]).reset_index()\n",
        "    df.columns=['sell_date','num_sold','products']\n",
        "    return df"
      ]
    }
  ]
}