        "import pandas as pd\n",
        "\n",
        "def order_scores(scores: pd.DataFrame) -> pd.DataFrame:\n",
        "    scores['rank'] = scores['score'].rank(ascending=False,method='dense')\n",
        "    return scores[['score','rank']].sort_values(by='score',ascending=False)"
      ]
