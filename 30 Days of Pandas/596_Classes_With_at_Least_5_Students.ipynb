        "import pandas as pd\n",
        "\n",
        "def find_classes(courses: pd.DataFrame) -> pd.DataFrame:\n",
        "    res= courses.groupby('class').count().reset_index()\n",
        "    res= res[res['student']>=5]\n",
        "    return pd.DataFrame(res['class'])"
